                        <label asp-for="Fechafirma" class="control-label">Fecha Firma</label>
                        <input asp-for="Fechafirma" class="form-control" type="date" />
                        <span asp-validation-for="Fechafirma" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="nombregestoria" class="control-label">Nombre Gestoria</label>
                        <input asp-for="nombregestoria" class="form-control" />
                        <span asp-validation-for="nombregestoria" class="text-danger"></span>
                    </div>
