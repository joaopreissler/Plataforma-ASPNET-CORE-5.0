                    </div>
                    <div class="form-group">
                        <label asp-for="sector" class="control-label">Sector</label>
                        <input asp-for="sector" class="form-control" />
                        <span asp-validation-for="sector" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="convenio" class="control-label">Convenio</label>
                        <input asp-for="convenio" class="form-control" />
                        <span asp-validation-for="convenio" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="actividadprincial" class="control-label">Actividad Principal</label>
                        <input asp-for="actividadprincial" class="form-control" />
                        <span asp-validation-for="actividadprincial" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="CNAE" class="control-label"></label>
                        <input asp-for="CNAE" class="form-control" />
                        <span asp-validation-for="CNAE" class="text-danger"></span>
                    </div>
                    <div class="form-group form-check">
                        <label class="form-check-label">
                            <input class="form-check-input" asp-for="Representacionlegal" />Representación Legal
                        </label>
                    </div>
                    <div class="form-group form-check">
                        <label class="form-check-label">
                            <input class="form-check-input" asp-for="nuevacreacion" />Nueva Creacion
                        </label>
                    </div>
                    <div class="form-group">
                        <label asp-for="creditodisponible" class="control-label">Crédito Disponible</label>
                        <input asp-for="creditodisponible" class="form-control" />
                        <span asp-validation-for="creditodisponible" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Banco" class="control-label"></label>
                        <input asp-for="Banco" class="form-control" />
                        <span asp-validation-for="Banco" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Cuenta" class="control-label">Cuenta</label>
                        <input asp-for="Cuenta" class="form-control" />
                        <span asp-validation-for="Cuenta" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="creacion" class="control-label">Fecha de creación de la Empresa</label>
                        <input asp-for="creacion" class="form-control" type="date" />
                        <span asp-validation-for="creacion" class="text-danger"></span>
                    </div>
                    <div class="form-group form-check">
                        <label class="form-check-label">
                            <input class="form-check-input" asp-for="venta" />Venta
                        </label>
                    </div>
