                        <label asp-for="emailgestoria" class="control-label">Email Gestoria</label>
                        <input asp-for="emailgestoria" class="form-control" />
                        <span asp-validation-for="emailgestoria" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="telefonogestoria" class="control-label">Teléfono Gestoria</label>
                        <input asp-for="telefonogestoria" class="form-control" />
                        <span asp-validation-for="telefonogestoria" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="ano" class="control-label">Año</label>
                        <input asp-for="ano" class="form-control" />
                        <span asp-validation-for="ano" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Idcurso" class="control-label">Mes</label>
                        <select asp-for="Idcurso" name="Idcurso" id="mytable">

                            <option value=1>Enero</option>
                            <option value=2>Febrero</option>
                            <option value=3>Marzo</option>
                            <option value=4>Abril</option>
                            <option value=5>Mayo</option>
                            <option value=6>Junio</option>
                            <option value=7>Julio</option>
                            <option value=8>Agosto</option>
                            <option value=9>Septiembre</option>
                            <option value=10>Octubre</option>
                            <option value=11>Noviembre</option>
                            <option value=12>Deciembre</option>

                        </select>
                        <span asp-validation-for="Idcurso" class="text-danger"></span>
                    </div>
                </div>
              
            </div>
            <div class="form-group">
                <input type="submit" value="Añadir Empresa" class="btn btn-primary" />
            </div>

        </form>
    </div>
</div>



<div>
    <a asp-action="Index">Volver</a>
</div>


    </div>
